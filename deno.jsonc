{
  "tasks": {
    "start": "deno --unstable run -A --watch=static/,routes/ dev.ts",
    "pack": "deno fmt && deno lint && deno task start",
    "esm:add": "deno run -A https://esm.sh/v124 add",
    "esm:update": "deno run -A https://esm.sh/v124 update",
    "esm:remove": "deno run -A https://esm.sh/v124 remove"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "fmt": {
    "semiColons": false,
    "singleQuote": true
  },
  "imports": {
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.3.1",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.5",
    // "@tanstack/query-core": "https://esm.sh/@tanstack/query-core@4.29.19?bundle",
    "@tanstack/react-query": "https://esm.sh/@tanstack/react-query@4.29.19?external=react",
    "@trpc/client": "https://esm.sh/@trpc/client@10.34.0",
    "@trpc/server": "https://esm.sh/@trpc/server@10.34.0",
    "@trpc/server/": "https://esm.sh/@trpc/server@10.34.0/",
    "$fresh/": "https://deno.land/x/fresh@1.3.0/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.0",
    // Make sure these versions are the same -->
    "preact": "https://esm.sh/preact@10.15.1",
    "preact/": "https://esm.sh/preact@10.15.1/",
    "react": "https://esm.sh/preact@10.15.1/compat",
    // <--
    "trpc-react-query": "https://esm.sh/*@trpc/react-query@10.29.0",
    "@twind/": "https://esm.sh/@twind/",
    // "use-sync-external-store": "https://esm.sh/*use-sync-external-store@1.2.0?bundle",
    "zod": "https://esm.sh/zod@3.21.4",
    "kvdex": "https://deno.land/x/kvdex@v0.5.3/mod.ts",
    "kv_oauth": "https://deno.land/x/deno_kv_oauth@v0.2.4/mod.ts"
  },
  "scopes": {}
}
